<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lnData"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="5dp"
        android:text="Chi tiết bảng tổng hợp"
        android:textColor="@color/back_1"
        android:textSize="16sp" />

    <TableLayout
        android:id="@+id/tbTotalDetail"
        android:stretchColumns="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    </TableLayout>

    <LinearLayout
        android:id="@+id/lnDetail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/lnDe"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingEnd="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Đặc biệt (Đề)"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbDe"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                </TableLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lnLo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingStart="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Lô"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbLo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                </TableLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="12dp"
            android:id="@+id/lnXien"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_header"
                android:gravity="center"
                android:text="Xiên"
                android:textColor="@color/back_1"
                android:textSize="15sp"
                  android:fontFamily="sans-serif-medium" />

            <TableLayout
                android:id="@+id/tbXien"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lnBaCang"
            android:layout_marginTop="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_header"
                android:gravity="center"
                android:text="Ba càng"
                android:textColor="@color/back_1"
                android:textSize="15sp"
                  android:fontFamily="sans-serif-medium" />

            <TableLayout
                android:id="@+id/tbBaCang"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/lnDuoiG1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingEnd="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Đuôi G1"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbDuoiG1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                </TableLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lnCangGiua"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingStart="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Càng giữa"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbCangGiua"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                </TableLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/lnDauG1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingEnd="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Đầu G1"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbDauG1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                </TableLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lnDauDB"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:paddingStart="3dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_header"
                    android:gravity="center"
                    android:text="Dầu DB"
                    android:textColor="@color/back_1"
                    android:textSize="15sp"
                      android:fontFamily="sans-serif-medium" />

                <TableLayout
                    android:id="@+id/tbDauDB"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                </TableLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>